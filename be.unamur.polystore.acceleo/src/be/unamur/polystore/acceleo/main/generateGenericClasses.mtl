		if(this == NOT_EQUALS)
			return "$ne";
		if(this == GT)
			return "$gt";
		if(this == GTE)
			return "$gte";
		if(this == LT)
			return "$lt";
		if(this == LTE)
			return "$lte";
		if(this == CONTAINS)
			return "$regex";
		/**if(this == START_WITH)
			return "LIKE";
		if(this == END_WITH)
			return "LIKE";**/
		
